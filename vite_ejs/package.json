{
  "name": "vite_ejs_template",
  "description": "Vite + ejs + Sass + Babel",
  "version": "1.4.0",
  "keywords": [
    "Vite",
    "ejs",
    "scss",
    "babel"
  ],
  "author": "e@oshinco.com",
  "license": "Oshinco Co-op",
  "scripts": {
    "start": "yarn images:start && yarn lint && vite --mode watch",
    "preview": "yarn images:start && yarn lint && vite preview --mode production ",
    "dev": "yarn clean && vite build --mode development && yarn html && yarn images:build && yarn style:media",
    "build": "yarn clean && vite build --mode production  && yarn html && yarn images:build && yarn style:media",
    "deploy": "",
    "html": "node htmlBeautify.mjs",
    "html:lint": "markuplint \"src/**/{*.ejs,*.html}\"",
    "style:lint": "",
    "style:media": "node mediaQueries.mjs",
    "es:lint": "eslint \"src/**/*.js\"",
    "lint": "yarn es:lint --fix",
    "images:build": "node convertImage.mjs -i ./src/images -o ./dist/assets/images -m -w -t -v",
    "images:start": "node convertImage.mjs -i ./src/images -o ./src/public/assets/images -m -w -t -v",
    "images:start_": "node convertImage.mjs -i ./src/images -o ./.yarn/temp/public/assets/images -m -w -t -v",
    "ln": "node createSymlink.mjs",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-arrow-functions": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.6",
    "@eslint/js": "^9.29.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@img/sharp-win32-x64": "^0.34.2",
    "@markuplint/ejs-parser": "^4.6.19",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "autoprefixer": "^10.4.21",
    "babel-plugin-import": "^1.13.8",
    "commander": "^14.0.0",
    "css-declaration-sorter": "^7.2.0",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "ejs-lint": "^2.0.1",
    "eslint": "^9.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "globals": "^16.2.0",
    "js-beautify": "^1.15.4",
    "markuplint": "^4.12.0",
    "npm-check-updates": "^18.0.1",
    "postcss": "^8.5.6",
    "postcss-normalize-charset": "^7.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.6.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.0",
    "sass-embedded": "^1.89.2",
    "sharp": "^0.34.2",
    "svgo": "^4.0.0",
    "vite": "^7.0.0",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-live-reload": "^3.0.4"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "supports-color": "8.1.1",
    "tailwindcss": "^4.1.10"
  },
  "browserslist": [
    "last 3 versions",
    "ie >= 11"
  ],
  "comments": {
    "VSCode SDK セットアップ": "$ yarn dlx @yarnpkg/sdks vscode",
    "npm-check-updates": "$ yarn ncu, $ yarn ncu -u, $ yarn ncu --upgrade --target minor, $ yarn ncu -u --target patch",
    "yarn": "yarn cache clean",
    "NOTE": "supports-color@8.1.1 を使用。debug等が10以降を要求してもアップデート不可。ESM化の影響を避けるため。"
  }
}
