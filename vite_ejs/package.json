{
  "name": "vite_ejs_template",
  "description": "Vite + ejs + Sass + Babel",
  "version": "1.2.0",
  "keywords": [
    "Vite",
    "ejs",
    "scss",
    "babel"
  ],
  "author": "eiji@design24c.com",
  "license": "(c) Design24C",
  "scripts": {
    "start": "yarn images && yarn lint && cross-env NODE_ENV=preview vite",
    "preview": "yarn images && yarn lint && cross-env NODE_ENV=production vite preview",
    "dev": "rm -rf dist && cross-env NODE_ENV=development vite build && yarn html && yarn images",
    "build": "rm -rf dist && cross-env NODE_ENV=production vite build && yarn html && yarn images",
    "deploy": "",
    "html": "yarn html:lint && node htmlBeautify.mjs",
    "html:lint": "markuplint \"src/**/{*.ejs,*.html}\"",
    "style:lint": "",
    "es:lint": "eslint \"src/**/*.js\"",
    "lint": "yarn es:lint --fix && yarn html:lint",
    "images": "node convertImage.mjs -i ./src/public/assets/images -o ./dist/assets/images -m -w -t -v",
    "clean": "rimraf build"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-arrow-functions": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/runtime": "^7.24.4",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@markuplint/ejs-parser": "^4.5.1",
    "@rollup/plugin-babel": "^6.0.4",
    "autoprefixer": "^10.4.19",
    "babel-plugin-import": "^1.13.8",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^7.2.0",
    "ejs": "^3.1.10",
    "ejs-lint": "^2.0.0",
    "eslint": "^9.1.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "globals": "^15.0.0",
    "js-beautify": "^1.15.1",
    "markuplint": "^4.6.1",
    "npm-check-updates": "^16.14.18",
    "postcss": "^8.4.38",
    "postcss-normalize-charset": "^6.0.2",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.16.4",
    "sass": "^1.75.0",
    "sharp": "^0.33.3",
    "svgo": "^3.2.0",
    "vite": "^5.2.10",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-live-reload": "^3.0.3"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "tailwindcss": "^3.4.3"
  },
  "browserslist": [
    "last 3 versions",
    "ie >= 11"
  ],
  "comments": {
    "eslint-plugin-react": "^7.34.1",
    "": "",
    "npm-check-updates": "$ yarn ncu, $ yarn ncu -u --target patch",
    "yarn": "yarn cache clean"
  }
}
