{
  "name": "vite_ejs_template",
  "description": "Vite + ejs + Sass + Babel",
  "version": "1.1.0",
  "keywords": [],
  "author": "eiji@design24c.com",
  "license": "(c) Design24C",
  "scripts": {
    "start": "yarn images && vite",
    "dev": "yarn images && vite",
    "_build": "rm -rf dist && vite build && html-beautify dist/**/*.html && esbuild src/public/assets/js/*.js --bundle --minify --outdir=dist/assets/js/",
    "build": "rm -rf dist && vite build && yarn html && yarn images",
    "preview": "vite preview",
    "deploy": "",
    "html": "html-beautify dist/**/*.html",
    "images": "node convertImage.mjs -i ./src/public/assets/images -o ./dist/assets/images -m -w -t -v",
    "clean": "rimraf build"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-arrow-functions": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/runtime": "^7.24.4",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "autoprefixer": "^10.4.19",
    "babel-plugin-import": "^1.13.8",
    "commander": "^12.0.0",
    "css-declaration-sorter": "^7.2.0",
    "ejs": "^3.1.10",
    "ejs-lint": "^2.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.34.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "js-beautify": "^1.15.1",
    "npm-check-updates": "^16.14.18",
    "postcss": "^8.4.38",
    "postcss-normalize-charset": "^6.0.2",
    "postcss-sort-media-queries": "^5.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.3",
    "sass": "^1.75.0",
    "sharp": "^0.33.3",
    "svgo": "^3.2.0",
    "vite": "^5.2.9",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-live-reload": "^3.0.3"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "tailwindcss": "^3.4.3"
  },
  "browserslist": [
    "last 3 versions",
    "ie >= 11"
  ],
  "comments": {
    "npm-check-updates": "$ yarn ncu, $ yarn ncu -u --target patch",
    "yarn": "yarn cache clean"
  }
}
