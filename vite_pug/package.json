{
  "name": "vite_pug_template",
  "description": "Vite + Pug + Sass + Babel",
  "version": "1.3.0",
  "keywords": [
    "Vite",
    "pug",
    "scss",
    "babel"
  ],
  "author": "e@oshinco.com",
  "license": "Oshinco Co-op",
  "scripts": {
    "start": "yarn images:start && yarn lint && vite --mode watch",
    "preview": "yarn images:start && yarn lint && vite preview --mode production",
    "dev": "yarn clean && vite build --mode development && yarn html && yarn images:build && yarn style:media",
    "build": "yarn clean && vite build --mode production && yarn html && yarn images:build && yarn style:media",
    "deploy": "",
    "html": "node htmlBeautify.mjs",
    "html:lint": "markuplint \"src/**/{*.pug,*.html}\"",
    "style:lint": "",
    "style:media": "node mediaQueries.mjs",
    "es:lint": "eslint \"src/**/*.js\"",
    "lint": "yarn es:lint --fix",
    "images:build": "node convertImage.mjs -i ./src/images -o ./dist/assets/images -m -w -t -v",
    "images:start": "node convertImage.mjs -i ./src/images -o ./.yarn/temp/public/assets/images -m -w -t -v",
    "ln": "node createSymlink.mjs",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-arrow-functions": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@img/sharp-win32-x64": "^0.33.5",
    "@markuplint/pug-parser": "^4.6.13",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/node": "^22.10.0",
    "@types/pug": "^2.0.10",
    "autoprefixer": "^10.4.20",
    "commander": "^12.1.0",
    "css-declaration-sorter": "^7.2.0",
    "eslint": "^9.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "globals": "^15.12.0",
    "js-beautify": "^1.15.1",
    "markuplint": "^4.11.3",
    "npm-check-updates": "^17.1.11",
    "postcss": "^8.4.49",
    "postcss-normalize-charset": "^7.0.0",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.4.1",
    "pug": "^3.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.4",
    "sass-embedded": "^1.81.0",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "vite": "^6.0.1",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-live-reload": "^3.0.3"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "tailwindcss": "^3.4.15"
  },
  "browserslist": [
    "last 3 versions",
    "ie >= 11"
  ],
  "comments": {
    "": "",
    "npm-check-updates": "$ yarn ncu, $ yarn ncu -u, $ yarn ncu --upgrade --target minor, $ yarn ncu -u --target patch",
    "yarn": "yarn cache clean"
  }
}
