{
  "name": "vite-ejs-lite-template",
  "description": "Vite + ejs + Sass",
  "version": "1.4.9",
  "keywords": [
    "Vite",
    "ejs",
    "scss"
  ],
  "author": "e@oshinco.com",
  "license": "Oshinco Co-op",
  "private": true,
  "scripts": {
    "dev": "vite --mode watch",
    "start": "pnpm dev",
    "preview": "vite preview",
    "build:dev": "pnpm clean:dist && vite build --mode development && pnpm html && pnpm style:media",
    "build:prod": "pnpm clean:dist && vite build --mode production && pnpm html && pnpm style:media",
    "build": "pnpm build:prod",
    "html": "node htmlBeautify.mjs",
    "style:media": "node mediaQueries.mjs",
    "clean:cache": "pnpm store prune",
    "clean:dist": "rimraf dist",
    "clean:node": "rimraf node_modules",
    "clean:all": "pnpm clean:cache && pnpm clean:dist && pnpm clean:node",
    "update:list": "pnpm outdated",
    "update:dev": "pnpm update",
    "update:all": "pnpm update -L"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@vitejs/plugin-legacy": "^7.2.1",
    "autoprefixer": "^10.4.23",
    "css-declaration-sorter": "^7.3.1",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "js-beautify": "^1.15.4",
    "postcss": "^8.5.6",
    "postcss-merge-at-rules": "^1.2.0",
    "postcss-sort-media-queries": "^5.2.0",
    "rimraf": "^6.1.2",
    "sass": "^1.97.2",
    "terser": "^5.44.1",
    "vite": "npm:rolldown-vite@^7.3.1",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-live-reload": "^3.0.5"
  },
  "comments": {
    "security issues": "pnpm audit --audit-level high, (Low、Moderate、High、Critical)",
    "NOTE": ""
  },
  "dependencies": {
    "kiso.css": "^1.2.3"
  }
}
