{
  "name": "vite-ejs-lite-template",
  "description": "Vite + ejs + Sass + Babel",
  "version": "1.4.2",
  "keywords": [
    "Vite",
    "ejs",
    "scss",
    "babel"
  ],
  "author": "e@oshinco.com",
  "license": "Oshinco Co-op",
  "private": true,
  "scripts": {
    "start": "vite --mode watch",
    "dev": "yarn clean && vite --mode development && yarn html && yarn style:media",
    "build": "yarn clean && vite build --mode production && yarn html && yarn style:media",
    "preview": "vite preview",
    "html": "node htmlBeautify.mjs",
    "style:media": "node mediaQueries.mjs",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@vitejs/plugin-legacy": "^7.2.1",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "js-beautify": "^1.15.4",
    "npm-check-updates": "^19.1.2",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "sass": "^1.94.2",
    "terser": "^5.44.1",
    "vite": "^7.2.4",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-live-reload": "^3.0.5"
  },
  "comments": {
    "npm-check-updates": "$ yarn ncu, $ yarn ncu -u, $ yarn ncu --upgrade --target minor, $ yarn ncu -u --target patch",
    "yarn": "yarn cache clean",
    "NOTE": ""
  }
}
